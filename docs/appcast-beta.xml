        <item>
            <title>Version 1.3.0-beta.5</title>
            <pubDate>Fri, 28 Feb 2026 21:45:00 +0800</pubDate>
            <sparkle:version>13005</sparkle:version>
            <sparkle:shortVersionString>1.3.0-beta.5</sparkle:shortVersionString>
            <sparkle:minimumSystemVersion>13.0</sparkle:minimumSystemVersion>
            <description>
                &lt;h2&gt;VibeBar 1.3.0-beta.5&lt;/h2&gt;
                &lt;ul&gt;
                    &lt;li&gt;Redesigned update section with reduced visual weight&lt;/li&gt;
                    &lt;li&gt;Conditional beta warning display&lt;/li&gt;
                    &lt;li&gt;Confirmation dialog for reset to defaults&lt;/li&gt;
                    &lt;li&gt;Immediate session cleanup when toggling CLI tools&lt;/li&gt;
                    &lt;li&gt;Locked window height with scrolling&lt;/li&gt;
                    &lt;li&gt;Redesigned CLI tool list with install detection&lt;/li&gt;
                &lt;/ul&gt;
            </description>
            <enclosure url="https://github.com/yelog/VibeBar/releases/download/v1.3.0-beta.5/VibeBar-1.3.0-beta.5-universal.dmg" length="0" type="application/octet-stream" sparkle:edSignature="" sparkle:dsaSignature="" />
        </item>

        
